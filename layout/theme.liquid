            $(function(){
            const openBtn = $("#hamburger");
            const closeBtn = $("#close");
            const overlay = $("#overlay");
            const panel = $("#drawer-menu");
            function panelOpen() {
              overlay.fadeIn('fast');
              panel.addClass('open');
            }
            function panelClose() {
              overlay.fadeOut('fast');
              panel.removeClass('open');
            }
            openBtn.on('click',function(){
              panelOpen();
            });
            closeBtn.on('click',function(){
              panelClose();
            });
            overlay.on('click',function(){
              panelClose();
            });
            $(window).on('resize',function(){
              panelClose();
            });
          });

            $(".selector-wrapper.js.product-form__item span:nth-child(1)").addClass("checked");
            
            $(".selector-wrapper.js.product-form__item span").click(function(e){
              e.preventDefault();
              $(this).addClass("checked");
              var varintimg = $(this).attr("variant-img");
              var varianttitle = $(this).attr("vproduct-title");
              $(".cutt-cal-img").each(function(){
              var varianttitleimg = $(this).attr("product-title");
                if ( varianttitle == varianttitleimg ){
                 $(this).find("img").attr("src", varintimg);
                }
              });
              
            });

            $(".cust-main-collection .selector-wrapper.js.product-form__item").click(function(e){
             e.preventDefault();
            });
            
              $('.selector-wrapper.js.product-form__item span').on('click', function(event){
              event.preventDefault();
              $(this).addClass('variant-active').siblings().removeClass('variant-active');
            
            });

