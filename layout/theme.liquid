<script>
$(".itg-response").bind("DOMSubtreeModified", function() {
    $(this).parents('#mc_embed_signup').addClass('mc_embed_success');
  var this_html = $(this).text();
  $('.submit_button_subscribe').hide();
  setTimeout(function() { 
      $('#mc_embed_signup').removeClass('mc_embed_success');
      $('.itg-response').hide();
    $('.submit_button_subscribe').show();
    }, 5000);
  if (this_html === 'Thank you for subscribing!') {
    $(this).html('Welcome To The Bureau Fam! Check your email for a confirmation message.');
  }

  
});
</script>
    
